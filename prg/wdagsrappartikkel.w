  ASSIGN
      FI-Dato = TODAY - 1
      CB-Sort:LIST-ITEM-PAIRS IN FRAME {&FRAME-NAME} = cTekst
      CB-Sort = 1
      .

      RUN SUPER.

  /* Code placed here will execute AFTER standard behavior.    */
      RUN SwitchLng.

