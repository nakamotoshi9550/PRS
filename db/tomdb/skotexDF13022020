ADD TABLE "Garanti"
  AREA "Schema Area"
  DESCRIPTION "Garantiklasser"
  VALEXP "not can-find(first ArtBas where ArtBas.garantikl = garanti.garantikl)"
  VALMSG "Denne garantiklassen kan ikke slettes"
  DUMP-NAME "garanti"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg\c_garanti.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_garanti.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg\w_garanti.p" CRC "?" 

