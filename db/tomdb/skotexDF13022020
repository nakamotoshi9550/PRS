  FORMAT ">9"
  INITIAL "0"
  LABEL "Garantikl"
  POSITION 2
  MAX-WIDTH 4
  COLUMN-LABEL "Garantikl"
  HELP "Garantiklasse"
  ORDER 10
  MANDATORY

