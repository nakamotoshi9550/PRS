  AREA "Data Area"
  INDEX-FIELD "AarPerLinNr" ASCENDING 
  INDEX-FIELD "StTypeId" ASCENDING 
  INDEX-FIELD "DataObjekt" ASCENDING 
  INDEX-FIELD "Butik" ASCENDING 

