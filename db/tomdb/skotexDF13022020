  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_medlembettrans.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg\w_medlembettrans.p" CRC "?" 

