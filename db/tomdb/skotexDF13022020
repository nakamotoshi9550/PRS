  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Endret tid"
  POSITION 5
  MAX-WIDTH 4
  COLUMN-LABEL "ETid"
  HELP "Endret tidspunkt"
  ORDER 50

