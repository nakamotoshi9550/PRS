ADD TABLE "z_nummer"
  AREA "Data Area"
  DESCRIPTION "Inneholder z-nummer for hver kasse."
  DUMP-NAME "z_nummer"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg\c_z_nummer.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_z_nummer.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg\w_z_nummer.p" CRC "?" 

ADD FIELD "butikk" OF "z_nummer" AS integer 
  DESCRIPTION "Peker til butikkregister"
  FORMAT "z9"
  INITIAL "0"
  LABEL "Butikk"
  POSITION 2
  MAX-WIDTH 4
  ORDER 10

ADD FIELD "kasse" OF "z_nummer" AS integer 
  DESCRIPTION "Viser hvilken kasse z-nummeret gjelder."
  FORMAT "z9"
  INITIAL "0"
  LABEL "Kasse"
  POSITION 3
  MAX-WIDTH 4
  ORDER 20

ADD FIELD "z_nummer" OF "z_nummer" AS integer 
  FORMAT "zzz9"
  INITIAL "0"
  LABEL "Z-nummer"
  POSITION 4
  MAX-WIDTH 4
  ORDER 30

ADD INDEX "z_nummer" ON "z_nummer" 
  AREA "Index Area"
  UNIQUE
  PRIMARY
  INDEX-FIELD "butikk" ASCENDING 
  INDEX-FIELD "kasse" ASCENDING 

