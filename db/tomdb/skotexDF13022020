ADD TABLE "FakturaLinje"
  AREA "Data Area"
  LABEL "Fakturalinje"
  DESCRIPTION "ALle linjer til en faktura."
  DUMP-NAME "faktural"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg\c_FakturaLinje.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_FakturaLinje.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg\w_FakturaLinje.p" CRC "?" 

