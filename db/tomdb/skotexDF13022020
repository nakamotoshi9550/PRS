ADD TABLE "KundeBetTrans"
  AREA "Data Area"
  DESCRIPTION "Kundens betalingstransaksjoner"
  DUMP-NAME "kundebet"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg/c_kundebettrans.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_kundebettrans.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg/w_kundebettrans.p" CRC "?" 

ADD FIELD "BatchNr" OF "KundeBetTrans" AS integer 
  FORMAT "zzzzzzzz9"
  INITIAL "0"
  LABEL "BatchNummer"
  POSITION 2
  MAX-WIDTH 4
  COLUMN-LABEL "Batch"
  HELP "Batch nummer som holder sammen transaksjoner"
  ORDER 10

