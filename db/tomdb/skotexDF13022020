ADD INDEX "KassererDag" ON "kassererDag" 
  AREA "Index Area"
  UNIQUE
  PRIMARY
  INDEX-FIELD "ButikkNr" ASCENDING 
  INDEX-FIELD "Dato" ASCENDING 
  INDEX-FIELD "KassererNr" ASCENDING 
  INDEX-FIELD "z_nummer" ASCENDING 

ADD INDEX "Dato2" ON "kassererDag" 
  AREA "Index Area"
  INDEX-FIELD "Dato" ASCENDING 
  INDEX-FIELD "ButikkNr" ASCENDING 
  INDEX-FIELD "KassererNr" ASCENDING 

ADD INDEX "Kasserer" ON "kassererDag" 
  AREA "Index Area"
  INDEX-FIELD "ButikkNr" ASCENDING 
  INDEX-FIELD "KassererNr" ASCENDING 
  INDEX-FIELD "Dato" ASCENDING 

ADD TABLE "KassererDat"
  AREA "Index Area"
  DESCRIPTION "Salg pr kasserer fra dagsoppgjør"
  DUMP-NAME "kassdag0"

