ADD TABLE "Aktivitet"
  AREA "Data Area"
  LABEL "Aktiviteter"
  DESCRIPTION "Varegrupper kan kobles til aktiviteter via VgAkt"
  DUMP-NAME "aktivite"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg/c_aktivitet.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg/d_aktivitet.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg/w_aktivitet.p" CRC "?" 

ADD FIELD "Beskrivelse" OF "Aktivitet" AS character 
  FORMAT "X(30)"
  INITIAL ""
  LABEL "Beskrivelse"
  POSITION 2
  MAX-WIDTH 60
  HELP "Kort beskrivelse av aktivitet"
  ORDER 20

ADD FIELD "Merknad" OF "Aktivitet" AS character 
  FORMAT "X(50)"
  INITIAL ""
  LABEL "Merknad"
  POSITION 3
  MAX-WIDTH 100
  HELP "Merknad"
  ORDER 30

ADD FIELD "EDato" OF "Aktivitet" AS date 
  FORMAT "99/99/9999"
  INITIAL ?
  LABEL "Endret"
  POSITION 4
  MAX-WIDTH 4
  HELP "Endret dato"
  ORDER 40

