ADD TABLE "KOrdreLinje"
  AREA "Data Area"
  LABEL "KOrdreLinje"
  DESCRIPTION "ALle linjer til en faktura."
  DUMP-NAME "faktura2"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "trg\c_KOrdreLinje.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "trg\d_KOrdreLinje.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "trg\w_KOrdreLinje.p" CRC "?" 

